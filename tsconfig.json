{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@libs/cache": ["./libs/cache/src"],
      "@libs/cache/*": ["./libs/cache/src/*"],
      "@libs/consumers": ["./libs/consumers/src"],
      "@libs/consumers/*": ["./libs/consumers/src/*"],
      "@libs/deduplication": ["./libs/deduplication/src"],
      "@libs/deduplication/*": ["./libs/deduplication/src/*"],
      "@libs/events": ["./libs/events/src"],
      "@libs/events/*": ["./libs/events/src/*"],
      "@libs/idempotency": ["./libs/idempotency/src"],
      "@libs/idempotency/*": ["./libs/idempotency/src/*"],
      "@libs/lock": ["./libs/lock/src"],
      "@libs/lock/*": ["./libs/lock/src/*"],
      "@libs/publishers": ["./libs/publishers/src"],
      "@libs/publishers/*": ["./libs/publishers/src/*"],
      "@libs/redis-lock": ["./libs/redis-lock/src"],
      "@libs/redis-lock/*": ["./libs/redis-lock/src/*"],
      "@libs/sns": ["./libs/sns/src"],
      "@libs/sns/*": ["./libs/sns/src/*"],
      "@libs/sns-publishers": ["./libs/sns-publishers/src"],
      "@libs/sns-publishers/*": ["./libs/sns-publishers/src/*"],
      "@libs/sqs": ["./libs/sqs/src"],
      "@libs/sqs/*": ["./libs/sqs/src/*"],
      "@libs/sqs-consumers": ["./libs/sqs-consumers/src"],
      "@libs/sqs-consumers/*": ["./libs/sqs-consumers/src/*"],
      "@libs/health": ["./libs/health/src"],
      "@libs/health/*": ["./libs/health/src/*"],
      "@libs/transactions": ["./libs/transactions/src"],
      "@libs/transactions/*": ["./libs/transactions/src/*"],
      "@libs/mongodb-transactions": ["./libs/mongodb-transactions/src"],
      "@libs/mongodb-transactions/*": ["./libs/mongodb-transactions/src/*"],
      "@libs/logging": ["./libs/logging/src"],
      "@libs/logging/*": ["./libs/logging/src/*"],
      "@libs/date": ["./libs/date/src"],
      "@libs/date/*": ["./libs/date/src/*"],
      "@libs/database-tools": ["./libs/database-tools/src"],
      "@libs/database-tools/*": ["./libs/database-tools/src/*"],
      "@libs/mongodb-tools": ["./libs/mongodb-tools/src"],
      "@libs/mongodb-tools/*": ["./libs/mongodb-tools/src/*"],
      "@libs/validation": ["./libs/validation/src"],
      "@libs/validation/*": ["./libs/validation/src/*"],
      "@libs/types": ["./libs/types/src"],
      "@libs/types/*": ["./libs/types/src/*"],
      "@libs/modes": ["./libs/modes/src"],
      "@libs/modes/*": ["./libs/modes/src/*"],
      "@libs/blob-storage": ["./libs/blob-storage/src"],
      "@libs/blob-storage/*": ["./libs/blob-storage/src/*"],
      "@libs/s3-blob-storage": ["./libs/s3-blob-storage/src"],
      "@libs/s3-blob-storage/*": ["./libs/s3-blob-storage/src/*"],
      "@libs/telegram": ["./libs/telegram/src"],
      "@libs/telegram/*": ["./libs/telegram/src/*"],
      "@apps/polytetris/*": ["./apps/polytetris/src/*"]
    }
  }
}
